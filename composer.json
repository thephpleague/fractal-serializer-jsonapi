{
    "name": "league/fractal-serializer-jsonapi",
    "description": "Handle the output of complex data structures ready for API output.",
    "keywords": [
        "league",
        "api",
        "json",
        "rest",
        "fractal"
    ],
    "homepage": "http://fractal.thephpleague.com/",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-json": "*"
    },
    "require-dev": {
        "league/fractal": "0.20.x-dev|>0.20",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~9.0|~8.0|~5.5",
        "squizlabs/php_codesniffer": "~3.4"
    },
    "suggest": {
        "league/fractal": "The engine that powers this serializer"
    },
    "autoload": {
        "psr-4": {
            "League\\Fractal\\Serializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Fractal\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.20.x-dev"
        }
    },
    "conflict": {
        "league/fractal": "<1.0 <0.20"
    }
}
