{
    "name": "league/fractal-serializer-jsonapi",
    "description": "Handle the output of complex data structures ready for API output.",
    "keywords": [
        "league",
        "api",
        "json",
        "rest",
        "fractal"
    ],
    "homepage": "http://fractal.thephpleague.com/",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpstan/phpstan": "^0.11.8",
        "phpunit/phpunit": "~8.0",
        "squizlabs/php_codesniffer": "~3.4",
        "league/fractal": "~1.0|1.0.x-dev"
    },
    "suggest": {
        "league/fractal": "The engine that powers this serializer"
    },
    "autoload": {
        "psr-4": {
            "League\\Fractal\\Serializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Fractal\\Test\\": "test"
        }
    }
}
